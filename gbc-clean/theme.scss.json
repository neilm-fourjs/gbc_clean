{
	"theme-disable-context-menu": false,

	"gbc-ChromeBarWidget-height": "22px",
	"gbc-ChromeBarWidget-visible-items": "about uploadStatus runtimeStatus settings",

	"theme-font-family": "'Roboto, Helvetica, sans-serif'",
	"theme-font-size-ratio": ".9",
	"theme-margin-ratio": ".8",
	"theme-combobox-padding": "0px",
	"theme-sidebar-always-visible-min-width": "1921px",
	"theme-host-action-default-height": "26px",
	"theme-field-default-height": "26px",
	"theme-message-display-time": 10,
	"theme-error-display-time": 20,

	"gbc-TableWidget-margin-ratio": ".4",
	"gbc-TopMenuWidget-vertical-padding": "4px",
	"gbc-TopMenuWidget-horizontal-padding": "8px",
	"gbc-TopMenuWidget-line-height": "18px",

	"mt-field-border-width-top": "1px",
	"mt-field-border-width-right": "1px",
	"mt-field-border-width-bottom": "1px",
	"mt-field-border-width-left": "1px",
	"mt-button-margin-ratio": ".6",
	"mt-progress-height": "20px",

	"njm-toolbar-padding": "6px",

	"palette-primary": "$mt-deep-purple",
	"palette-secondary": "$mt-grey",
	"palette-text-secondary": "$mt-black",

	"theme-primary-background-color": "$palette-primary-800",
	"theme-primary-emphasis-background-color": "$palette-primary-900",
