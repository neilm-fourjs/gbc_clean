	"mt-field-border-color": "$theme-primary-background-color",

	"gbc-ButtonWidget-color": "$mt-white",
	"mt-button-primary-emphasis-background-color": "$palette-primary-700",
	"mt-button-primary-background-color": "$palette-primary-600",
	"mt-button-primary-color": "$mt-white",
	"mt-button-disabled-color": "$palette-primary-100",

	"mt-toolbar-background-color": "$palette-primary-900",

	"mt-progress-background-color": "$mt-white",
	"mt-progress-color": "$palette-primary-400",

	"njm-button-border-color": "$palette-primary-200"
