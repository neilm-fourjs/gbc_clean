{
  "theme-font-family": "'\"Droid Sans\", Helvetica, Arial, sans-serif'",
  "theme-font-size-ratio": "1",
  "theme-margin-ratio": "1",
  "theme-combobox-padding": "0px",
  "theme-sidebar-always-visible-min-width": "1601px",
	"theme-host-action-default-height": "26px",
	"gbc-ChromeBarWidget-height": "26px",
	"mt-field-default-height": "32px",
  "mt-field-border-width-top": "1px",
  "mt-field-border-width-right": "1px",
  "mt-field-border-width-bottom": "1px",
  "mt-field-border-width-left": "1px",

  "palette-primary": "$mt-blue-grey",
  "palette-secondary": "$mt-grey",
  "palette-text-secondary": "$mt-black",
  "theme-primary-background-color": "$palette-primary-800",
  "theme-primary-emphasis-background-color": "$palette-primary-900",
  "theme-primary-faded-background-color": "$palette-primary-100",
  "theme-primary-color": "$mt-white",
  "theme-secondary-color": "$palette-text-secondary-100",
  "theme-header-color": "$palette-secondary-200",

  "gbc-TableWidget-unfocused-currentRow-background-color": "$theme-header-color",
  "theme-currentrow-background-color": "$palette-secondary-400",
  "theme-currentrow-color": "$palette-text-secondary",

	"mt-field-border-color": "$theme-primary-background-color"

}
